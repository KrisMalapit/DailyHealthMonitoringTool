function verifyEmployeeIds(v) {
        if ($(v).val() == "") {
            $("#txtEmployeeName").val("");
            return false;
        }
        //return false;
         $.ajax({
                    url: "@Url.Action("GetEmpDetails","Home")"
                    , method: "POST"
                    , data: {EmpId : $(v).val()}
                    , dataType: 'json'
                    , success: function (responsedata) {
                        console.log(responsedata);
                        if (responsedata.status == "success") {
                            
                            if (responsedata.hasValue == "true") {
                               
                                $("#txtEmployeeName").val(responsedata.empfname + " " + responsedata.emplastname) ;
                               


                            } else {
                                $("#txtEmployeeName").val("");
                                $.notify({
                                    icon: "add_alert",
                                    message: "<b>Fail</b> - " + "No record found for the particular employees"

                                },
                                    {
                                        type: "danger",
                                        timer: 1000,
                                        placement: {
                                            from: "top",
                                            align: "center"
                                        }
                                    });
                            }


                        } else {
                            $("#txtEmployeeName").val("");
                             $.notify({
                                    icon: "add_alert",
                                    message: "<b>Fail</b> - " + responsedata.message

                                },
                                    {
                                        type: "danger",
                                        timer: 1000,
                                        placement: {
                                            from: "top",
                                            align: "center"
                                        }
                                    });
                        }
                    }
                })
        }
        $("#aReset").click(function () {
            $('#modalDataPrivacy').modal('hide');
        })
        $("#btnReset").click(function () {
            if ($('#txtEmployeeName').val() == "") {
                $.notify({
                    icon: "add_alert",
                    message: "<b>Fail</b> - " + "No record found for the particular employee"
                },
                    {
                        type: "danger",
                        timer: 1000,
                        placement: {
                            from: "top",
                            align: "center"
                        }
                    });
                return false;
            }
            if ($('#txtRemarks').val() == "") {
                $.notify({
                    icon: "add_alert",
                    message: "<b>Fail</b> - " + "Fill Remarks"
                },
                    {
                        type: "danger",
                        timer: 1000,
                        placement: {
                            from: "top",
                            align: "center"
                        }
                    });
                return false;
            }

             $.ajax({
                    url: "@Url.Action("UpdateQuarantineStatus","Home")"
                    , method: "POST"
                    , data: { EmployeeId: $("#txtEmployeeId").val(), Remarks: $("#txtRemarks").val()}
                    , dataType: 'json'
                    , success: function (responsedata) {

                        if (responsedata.status == "success") {

                             $.notify({
                                    icon: "add_alert",
                                    message: "<b>Success</b> Quarantine updated!"

                                },
                                    {
                                        type: "success",
                                        timer: 1000,
                                        placement: {
                                            from: "top",
                                            align: "center"
                                        }
                                 });
                         
                           //$("#modalUpdate").modal('hide');


                        } else {
                             $.notify({
                                    icon: "add_alert",
                                    message: "<b>Fail</b> - " + responsedata.message

                                },
                                    {
                                        type: "danger",
